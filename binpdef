	   \[A=>UP:\[B=>DOWN:\[C=>RIGHT:\[D=>LEFT:\[H=>HOME:\
	   \[M\ =>ABSPOS:\[M!=>MRKPOS:\[M"=>MRKAT:\
	   OA=>UP:OB=>DOWN:OC=>RIGHT:OD=>LEFT:\
	   OP=>COPEN:OQ=>DO:OR=>FSET:OS=>PSRCH:\
	   Ow=>CINS:Ox=>CDEL:Oy=>CPUT:Om=>CPICK:\
	   Ot=>LINS:Ou=>LDEL:Ov=>LPUT:Ol=>LPICK:\
	   Oq=>COPEN:Or=>PSRCH:Os=>MSRCH:\
	   Op=>FSET:On=>DO:OM=>ENTER:\
	   q=>1DEL:a=>1PUT:z=>1PICK:\
	   w=>2DEL:s=>2PUT:x=>2PICK:\
	   e=>3DEL:d=>3PUT:c=>COMMAND:\
	   r=>4DEL:f=>FSET:v=>4PICK:\
	   t=>5DEL:g=>5PUT:b=>5PICK:\
	   y=>PBSRCH:u=>MBSRCH:\
	   h=>HOME:j=>MFSRCH:\
	   n=>PRSRCH:m=>MRSRCH:\
	   i=>2TAB:o=>2BTAB:\
	   k=>KNOCK:l=>INDENT:\
	   ,=>4TAB:.=>4BTAB:\
	   p=>5TAB:;=>5BTAB:\
	   \-=>VTAB:\==>BHOME:\`=>BRET:\
	   \'=>HELP:\\=>SYSTEM:\
	   \/=>PARM:\
	   ^i=>SAVE:\
	   ^\177=>ABORT:^\h=>ABORT:\
	   1=>LSLIDE:2=>RSLIDE:3=>RSTRT:4=>SHOW:5=>BSAVE:\
	   6=>BRSTR:7=>FMAKE:8=>FALT:9=>FNEXT:0=>LRESET:

pclinux|linux using std pc keyboard:\
	\0=>ENTER:^a=>MARK:^b=>MPAGE:^c=&:\
	^d=>EXIT:^e=>GOEND:^f=>1TAB:^g=>GOTO:\
	^h=>BS:^i=>0TAB:^j=>RET:^k=>0BTAB:\
	^l=>INDENT:^m=>RET:^n=>JOIN:^o=>LOPEN:\
	^p=>PSRCH:^q=>MSRCH:^r=>PLINE:^s=&:\
	^t=>MLINE:^u=>UNDO:^v=>PPAGE:^w=>1BTAB:\
	^x=>1DEL:^y=&:^z=>STOP:\E=&:\
	^\=>COPEN:^]=>CINS:\36=>GOBEG:^_=>NL:\
	\177=>ABORT:\
	@@:@MOUSE=>ABSPOS:@MOUSE1=>ABSPOS:@MOUSE2=>MRKPOS:@MOUSE3=>MRKAT:\
	@BACKSPACE=>BS:@PPAGE=>PPAGE:@NPAGE=>MPAGE:@HOME=>HOME:\
	@END=>GOEND:@ENTER=>ENTER:@DC=>CDEL:@IC=>CINS:\
	@DOWN=>DOWN:@UP=>UP:@LEFT=>LEFT:@RIGHT=>RIGHT:\
	@F1=>COPEN:@F2=>DO:@F3=>FSET:@F4=>PSRCH:\
	@F5=>MSRCH:@F6=>CPICK:@F7=>CPUT:@F8=>CDEL:\
	@F9=>LPICK:@F10=>LPUT:@F11=>LDEL:@F12=>LINS:\
	@A1=>CINS:\
	$HUP=>CRASH:$INT=?>ABORT:$TERM=>CRASH:$QUIT=>COPEN:\
	&^s:w=>1PUT:l=>LPUT:c=>CPUT:p=>2PUT:\
	&^y:w=>1PICK:l=>LPICK:c=>CPICK:p=>2PICK:\
	&^c:w=>1DEL:l=>LDEL:c=>CDEL:p=>2DEL:^c=>CDEL:^l=>LDEL:^w=>1DEL:^p=>2DEL:\
	&\E:\E\E=>RESET:\
	   \E\[11~=>ENTER:\E\[12~=>CINS:\E\[13~=>CDEL:\E\[14~=>CPUT:\E\[15~=>CPICK:\
	   \E\[16~=>LINS:\E\[17~=>LDEL:\E\[18~=>LPUT:\E\[19~=>LPICK:\
	   \E\[20~=>HOME:\
	   \[1~=>PRSRCH:\[2~=>1PUT:\[3~=>1DEL:\[4~=>1PICK:\
	   \[5~=>MPAGE:\[6~=>PPAGE:\
	   \[24~=>ENTER:\[25~=>PPAGE:\[26~=>MPAGE:\
	   \[28~=>PLINE:\[29~=>MLINE:\
	   \[31~=>2TAB:\[32~=>2BTAB:\
	   \[33~=>3TAB:\[34~=>3BTAB:\
	   \[11~=>COPEN:\[12~=>DO:\[13~=>FSET:\[14~=>PSRCH:\[15~=>MSRCH:\
	   \[17~=>LPICK:\[18~=>LPUT:\[19~=>LDEL:\[20~=>CPICK:\
	   \[21~=>CPUT:\[23~=>CDEL:\
	   \[35~=>CPUT:\[36~=>PSRCH:\[37~=>CPICK:\
	   \[38~=>LPUT:\[39~=>MSRCH:\[40~=>LPICK:\
	   \[41~=>CINS:\[43~=>CDEL:\[45~=>PPAGE:\
	   \[47~=>LINS:\[49~=>LDEL:\
	   \[42~=>UP:\[48~=>DOWN:\[46~=>RIGHT:\[44~=>LEFT:\
	   \[A=>UP:\[B=>DOWN:\[C=>RIGHT:\[D=>LEFT:\[H=>HOME:\
	   \[M\ =>ABSPOS:\[M!=>MRKPOS:\[M"=>MRKAT:\
	   OA=>UP:OB=>DOWN:OC=>RIGHT:OD=>LEFT:\
	   O2A=>UP:O2B=>DOWN:O2C=>RIGHT:O2D=>LEFT:\
	   O5A=>UP:O5B=>DOWN:O5C=>RIGHT:O5D=>LEFT:\
	   OP=>COPEN:OQ=>DO:OR=>FSET:OS=>PSRCH:\
